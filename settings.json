{
    "editor.fontFamily": "PlemolJP,HackGen,Cica,'BIZ UDGothic','UD デジタル 教科書体 N-R', Consolas, 'Courier New', monospace",
    "editor.fontSize": 15,
    "editor.useTabStops": true,
    "editor.renderWhitespace": "all",
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": true,
    "editor.glyphMargin": false,
    "editor.wordWrap": "off",
    "editor.guides.bracketPairs": "active",
    "editor.bracketPairColorization.enabled": true,
    "markdown-checkbox.checkmark": "x",
    "markdown-checkbox.dateFormat": "YYYY-MM-DD hh:mm:ss",
    "markdown-checkbox.italicWhenChecked": false,
    "npm.autoDetect": "off",
    "jake.autoDetect": "off",
    "gulp.autoDetect": "off",
    "grunt.autoDetect": "off",
    "security.workspace.trust.untrustedFiles": "open",
    "typescript.tsc.autoDetect": "off",
    "window.autoDetectHighContrast": false,
    "workbench.startupEditor": "none",
    "workbench.colorTheme": "Monokai",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "update.mode": "none",
    "files.exclude": {
        "**/{.git,.svn,.hg,CSV}": true,
        "**/.DS_Store":true,
        "**/{OBJ,OBJ_DEBUG}": true,
        "**/*.fossil": true
    },
    "files.watcherExclude": {
        "**/.git/**": true,
        "**/.svn/**": true
    },
    "[json]": {
        "editor.tabCompletion": "on",
        "editor.insertSpaces": true,
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace"
    },
    "[html]": {
        "editor.insertSpaces": true,
        "editor.wordWrap": "off",
        "editor.tabSize": 2,
        "editor.suggest.insertMode": "replace"
    },
    "[c]": {
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.tabSize": 4,
        "editor.useTabStops": true,
        "editor.insertSpaces": false,
        "files.encoding": "shiftjis"
    },
    "[cpp]": {
        "editor.tabSize": 4,
        "editor.useTabStops": true,
        "editor.insertSpaces": false,
        "files.encoding": "shiftjis"
    },
    "files.autoGuessEncoding": true,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "markup.strikethrough",
                "settings": {
                    "foreground": "#CCCCCC",
                    "fontStyle": "strikethrough"
                }
            }
        ]
    }
}