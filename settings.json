{
    "editor.accessibilitySupport": "off",
    "editor.bracketPairColorization.enabled": true,
    "editor.find.autoFindInSelection": "multiline",
    "editor.fontFamily": "'Cascadia Mono', 'BIZ UDGothic', 'UD デジタル 教科書体 N-R', Consolas, 'Courier New', monospace",
    "editor.fontSize": 13,
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.formatOnType": false,
    "editor.guides.bracketPairs": "active",
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "boundary",
    "editor.scrollbar.horizontal": "visible",
    "editor.scrollbat.vertical": "visible",
    "editor.snippetSuggestions": "top",
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.unicodeHighlight.includeComments": true,
    "editor.unicodeHighlight.invisibleCharacters": true,
    "editor.useTabStops": true,
    "editor.wordBasedSuggestions": true,
    "editor.wordBasedSuggestionsMode": "allDocuments",
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?、。「」【】『』（）！？",
    "editor.wordWrap": "off",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "markup.strikethrough",
                "settings": {
                    "foreground": "#AAAAAA",
                    "fontStyle": "strikethrough"
                }
            },
            {
                "scope": "punctuation.definition.heading.markdown",
                "settings": {
                    "fontStyle": "bold italic"
                }
            },
            {
                "scope": "punctuation.definition.list.begin.markdown",
                "settings": {
                    "foreground": "#E4007F",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "punctuation.definition.quote.begin.markdown",
                "settings": {
                    "foreground": "#4CBF99",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "markup.inline.raw.string.markdown",
                "settings": {
                    "foreground": "#ED9366"
                }
            },
            {
                "scope": "variable.language.this.cpp",
                "settings": {
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "search.mode": "newEditor",
    "search.searchOnType": false,
    "search.seedWithNearestWord": true,
    "search.showLineNumbers": true,
    "files.defaultLanguage": "{activeEditorLanguage}",
    "files.autoGuessEncoding": true,
    "files.exclude": {
        "**/{.git,.svn,.gh,CVS,.DS_Store}": true,
        "**/Thumbs.db": true,
        "**/{.vs,x64}": true,
        "**/*.{suo,user,exp,pdb,ilk}": true,
        "**/*.{exe,dll,lib}": true,
        "**/{OBJ,OBJ_DEBUG,Debug,Release}": true,
        "**/*.fossil": true
    },
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.shellIntegration.history": 25,
    "update.enableWindowsBackgroundUpdates": false,
    "update.mode": "none",
    "workbench.startupEditor": "none",
    "workbench.colorTheme": "Oranji",
    "workbench.editor.splitInGroupLayout": "vertical",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "[json]": {
        "editor.tabCompletion": "on",
        "editor.insertSpaces": true,
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace"
    },
    "[c]": {
        "editor.detectIndentation": false,
        "editor.tabSize": 4,
        "editor.useTabStops": true,
        "editor.insertSpaces": false,
        "editor.wordBasedSuggestions": true,
        "files.autoGuessEncoding": false,
        "files.encoding": "shiftjis"
    },
    "[cpp]": {
        "editor.detectIndentation": false,
        "editor.tabSize": 4,
        "editor.useTabStops": true,
        "editor.insertSpaces": false,
        "editor.wordBasedSuggestions": true,
        "files.autoGuessEncoding": false,
        "files.encoding": "shiftjis"
    },
    "[markdown]": {
        "editor.unicodeHighlight.invisibleCharacters": true,
        "editor.insertSpaces": false,
        "editor.detectIndentation" :false
    },
    "[planetext]": {
        "editor.unicodeHighlight.invisibleCharacters": true,
        "editor.detectIndentation" :false
    },
    "markdown-checkbox.checkmark": "x",
    "markdown-checkbox.dateFormat": "YYYY-MM-DD hh:mm:ss",
    "markdown-checkbox.italicWhenChecked": false,
    "C_Cpp.autocomplete": "Disabled",
    "C_Cpp.debugShortcut": false,
    "C_Cpp.default.browse.databaseFilename": "${env:TEMP}/vscode-cpptools",
    "C_Cpp.default.browse.limitSymbolsToIncludedHeaders": true,
    "C_Cpp.default.intelliSenseMode": "windows-msvc-x64",
    "C_Cpp.intelliSenseCachePath": "${env:TEMP}/vscode-cpptools",
    "C_Cpp.intelliSenseEngine": "Tag Parser",
    "vscode-neovim.neovimExecutablePaths.win32": "${env:HOME}\\scoop\\apps\\neovim\\current\\bin\\nvim.exe"
}
